<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         version="3.2"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd">

    <module-name>app-one</module-name>

    <enterprise-beans>
        <session>
            <ejb-name>AppOneRp</ejb-name>
            <business-remote>org.jboss.as.quickstarts.ejb.multi.server.app.AppOneRp</business-remote>
            <ejb-class>org.jboss.as.quickstarts.ejb.multi.server.app.AppOneRpBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
    </enterprise-beans>
</ejb-jar>